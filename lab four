# functions SumOflist() return the sum of a list of integers
def SumOflist(num_list):
  Sum = 0
  for i in range(len(num_list)):
      Sum+=num_list[i]
  return Sum

# function maxOflist() return the maxiumum of a list of intergers 
def maxOflist(num_list):
  Max = max(num_list)
  return Max

# functions minOflist() return the minimum of list a of integers 
def minOflist(num_list):
  Min = min(num_list)
  return Min

#functions  avgOflist() return the avaerage of a list of intergers 
def avgOflist(num_list):
    avg = sumOflist(num_list)/len(num_list)
    return avg

# Function concatenatesOflist() return the list of string after concatenates
def concatenatesOflist(string_list1, string1_list2):
  for i in string_list2:
      string_list1.append(i)
  return string_list1

#Declare list for intergers 
num_list = []

#Ask for list size
n= int(input("How many intgers do you want to enter :"))

#Ask for list intergers
print("Enter the list of intergers :")
for i in range(n):
  num=int(input())
  num_list.append(num)

#Test Functions 
print("Sum",SumOflist(num_list))
print("Max",maxOflist(num_list))
print("Min",minOflist(num_list))
#Declare list of strings 
string_list1 =[]
string_list2 =[]
#Ask for list 1 size
n1 = int(input("How many string do you want to enter in list1: "))

#Ask for list 1 elements
print("Enter the list 1 of strings :")
for i in range(n1):
    s=input()
    string_list1.append(s)

#Ask for list 2 size
n2=int(input("How many string do you want to enter in list 2: "))

#Ask for list 2 elements
print("Enter the list 2 of strings:")
for i in range(n2):
  s=input()
  string_list2.append(s)

#calling and print the list after concatenates
print("After concatenates: ",concatenatesOflist(string_list1, string_list2))  
